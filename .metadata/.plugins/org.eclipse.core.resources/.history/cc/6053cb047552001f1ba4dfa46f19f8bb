package com.aurionpro.tictactoe.model;

public class Players {
private String name;
private char choice;

public Players(String name, char choice) {
	super();
	this.name = name;
	this.choice = choice;
}
public String getName() {
	return name;
}
public void setName(String name) {
	this.name = name;
}
public char getChoice() {
	return choice;
}
public void setChoice(char choice) {
	this.choice = choice;
}

}
