package com.aurionpro.assignment.session22;

import java.util.Iterator;
import java.util.LinkedList;
import java.util.List;

public class Inventory {
private List<?> guitars;

public Inventory() {
	guitars = new LinkedList();
}

public void addGuitar(String serialNumber, String builder, String model, String type, String backlond, String topWood,
		double price) {
	Guitar guitar = new Guitar(serialNumber, builder, model, type, backlond, topWood,
			price);
	guitars.add(guitar);
}

