package com.aurionpro.structural.facade.model;

public class SoundSystem {
	private void start() {
		System.out.println("DVD Player is On Now.");
	}
	private void stop() {
		System.out.println("DVD Player is Off Now.");
	}
	
	private void volume(Volume level) {
		System.out.println("DVD Player is Off Now."+level);
	}
	
}
