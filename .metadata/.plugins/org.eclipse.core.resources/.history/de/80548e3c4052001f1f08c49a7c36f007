package com.aurionpro.tictactoe.test;


import com.aurionpro.tictactoe.model.MarkType;
import com.aurionpro.tictactoe.model.Players;

public class TicTacToeTest {
public static void main(String[] args) {
	MarkType board[][] = new MarkType[3][3];
	
	for(int i=0;i<3;i++) {
		for(int j=0;j<3;j++) {
			board[i][j] = MarkType.O;
		}
	}
	for(int i=0;i<3;i++) {
		for(int j=0;j<3;j++) {
			System.out.print(board[i][j]);
			System.out.print(" ");
		}
		System.out.println();
	}
	
	Players player1 = new Players("Player1", MarkType.X);
	Players player2 = new Players("Player2", MarkType.O);
	
}
}
