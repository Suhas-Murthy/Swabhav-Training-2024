package com.aurionpro.structural.facade.test;

public class DVDPlayer {
private void on() {
	System.out.println("DVD Player is On Now.");
}
private void Off() {
	System.out.println("DVD Player is Off Now.");
}
private void start() {
	System.out.println("DVD Player is started Now.");
}
private void stop() {
	System.out.println("DVD Player is stopped Now.");
}
}
