package com.aurionpro.iterativestructure;

public class ForLoopPrimeNumber {
public static void main(String[] args) {
	int number = 7;
	int count = 0;
	for (int i=2;i<number;i++) {
		if (number % i == 0) {
			count++;
		} 
//		i++;
	}
	if (count==0)
		System.out.println("Prime");
	else
		System.out.println("Not Prime");
}
}
